<h1>Listing tag_dreams</h1>

<table>
  <tr>
    <th>Tag</th>
    <th>Dream</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tag_dreams.each do |tag_dream| %>
  <tr>
    <td><%= tag_dream.tag %></td>
    <td><%= tag_dream.dream %></td>
    <td><%= link_to 'Show', tag_dream %></td>
    <td><%= link_to 'Edit', edit_tag_dream_path(tag_dream) %></td>
    <td><%= link_to 'Destroy', tag_dream, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Tag dream', new_tag_dream_path %>
